<?xml version="1.0"?>
<!--*- mode: xml -*-->
<interface>
  <object class="GtkWindow" id="PasswordEditor">
    <property name="default_height">250</property>
    <property name="default_width">440</property>
    <child>
      <object class="GtkTable" id="table2">
        <property name="border_width">5</property>
        <property name="column_spacing">6</property>
        <property name="n_columns">2</property>
        <property name="n_rows">2</property>
        <property name="visible">True</property>
        <child>
          <object class="GtkLabel" id="current_password_lbl">
            <property name="label" translatable="yes">Current Password:</property>
            <property name="visible">True</property>
            <property name="xalign">1.0</property>
          </object>
          <packing>
            <property name="x_options">fill</property>
            <property name="y_options">fill</property>
          </packing>
        </child>
        <child>
          <object class="GtkEventBox" id="password_holder">
            <property name="border_width">5</property>
            <property name="visible">True</property>
          </object>
          <packing>
            <property name="bottom_attach">2</property>
            <property name="right_attach">2</property>
            <property name="top_attach">1</property>
            <property name="x_options">fill</property>
            <property name="y_options">fill</property>
          </packing>
        </child>
        <child>
          <object class="ProxyEntry" id="current_password">
            <property name="data_type">unicode</property>
            <property name="mandatory">True</property>
            <property name="model_attribute">current_password</property>
            <property name="visibility">False</property>
            <property name="visible">True</property>
          </object>
          <packing>
            <property name="left_attach">1</property>
            <property name="right_attach">2</property>
            <property name="x_options">fill</property>
            <property name="y_options">fill</property>
          </packing>
        </child>
      </object>
    </child>
  </object>
</interface>
