<?xml version="1.0" standalone="no"?> <!--*- mode: xml -*-->
<!DOCTYPE glade-interface SYSTEM "http://gazpacho.sicem.biz/gazpacho-0.1.dtd">
<glade-interface domain="stoqlib">
    <widget class="GtkWindow" id="Warnbox">
        <property name="role"></property>
        <property name="title" context="yes" translatable="yes"></property>
        <property name="visible">True</property>
        <child>
            <widget class="GtkHBox" id="hbox1">
                <property name="visible">True</property>
                <child>
                    <widget class="GtkImage" id="error_icon">
                        <property name="file"></property>
                        <property name="stock">gtk-dialog-error</property>
                        <property name="visible">True</property>
                    </widget>
                    <packing>
                        <property name="expand">False</property>
                    </packing>
                </child>
                <child>
                    <widget class="GtkImage" id="alert_icon">
                        <property name="file"></property>
                        <property name="stock">gtk-dialog-warning</property>
                        <property name="visible">True</property>
                    </widget>
                    <packing>
                        <property name="expand">False</property>
                        <property name="position">1</property>
                    </packing>
                </child>
                <child>
                    <widget class="ProxyLabel" id="label">
                        <property name="data_type">unicode</property>
                        <property name="label" context="yes" translatable="yes">label</property>
                        <property name="visible">True</property>
                        <property name="xalign">0.02</property>
                    </widget>
                    <packing>
                        <property name="position">2</property>
                    </packing>
                </child>
            </widget>
        </child>
    </widget>
</glade-interface>
