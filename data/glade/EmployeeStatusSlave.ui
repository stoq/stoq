<?xml version="1.0"?>
<!--*- mode: xml -*-->
<interface>
  <object class="GtkWindow" id="EmployeeStatusSlave">
    <property name="can_focus">True</property>
    <property name="role"/>
    <property name="title" translatable="yes"/>
    <child>
      <object class="GtkVBox" id="vbox1">
        <property name="border_width">5</property>
        <property name="spacing">5</property>
        <property name="visible">True</property>
        <child>
          <object class="GtkHBox" id="hbox1">
            <property name="border_width">5</property>
            <property name="spacing">5</property>
            <property name="visible">True</property>
            <child>
              <object class="GtkLabel" id="label1">
                <property name="label" translatable="yes">Employee Status:</property>
                <property name="visible">True</property>
                <property name="xalign">1.0</property>
              </object>
            </child>
            <child>
              <object class="ProxyComboBox" id="statuses_combo">
                <property name="data_type">int</property>
                <property name="model_attribute">status</property>
                <property name="visible">True</property>
              </object>
              <packing>
                <property name="position">1</property>
              </packing>
            </child>
            <child>
              <object class="ProxyLabel" id="kiwilabel1">
                <property name="data_type">unicode</property>
                <property name="visible">True</property>
              </object>
              <packing>
                <property name="position">2</property>
              </packing>
            </child>
          </object>
        </child>
        <child>
          <object class="GtkHSeparator" id="hseparator1">
            <property name="visible">True</property>
          </object>
          <packing>
            <property name="expand">False</property>
            <property name="position">1</property>
          </packing>
        </child>
      </object>
    </child>
  </object>
</interface>
