<?xml version="1.0"?>
<!--*- mode: xml -*-->
<interface>
  <object class="GtkWindow" id="SellableTaxConstantEditor">
    <property name="default_height">250</property>
    <property name="default_width">440</property>
    <child>
      <object class="GtkTable" id="table">
        <property name="border_width">6</property>
        <property name="column_spacing">6</property>
        <property name="n_columns">2</property>
        <property name="n_rows">2</property>
        <property name="row_spacing">6</property>
        <property name="visible">True</property>
        <child>
          <object class="GtkLabel" id="label2">
            <property name="label" translatable="yes">Name:</property>
            <property name="visible">True</property>
            <property name="xalign">1.0</property>
          </object>
          <packing>
            <property name="x_options">fill</property>
          </packing>
        </child>
        <child>
          <object class="GtkLabel" id="label_value">
            <property name="label" translatable="yes">Value:</property>
            <property name="visible">True</property>
            <property name="xalign">1.0</property>
          </object>
          <packing>
            <property name="bottom_attach">2</property>
            <property name="top_attach">1</property>
            <property name="x_options">fill</property>
          </packing>
        </child>
        <child>
          <object class="ProxyEntry" id="description">
            <property name="data_type">unicode</property>
            <property name="mandatory">True</property>
            <property name="model_attribute">description</property>
            <property name="visible">True</property>
          </object>
          <packing>
            <property name="left_attach">1</property>
            <property name="right_attach">2</property>
            <property name="x_options">fill</property>
          </packing>
        </child>
        <child>
          <object class="ProxyEntry" id="tax_value">
            <property name="data_type">Decimal</property>
            <property name="mandatory">True</property>
            <property name="model_attribute">tax_value</property>
            <property name="visible">True</property>
            <property name="xalign">1.0</property>
          </object>
          <packing>
            <property name="bottom_attach">2</property>
            <property name="left_attach">1</property>
            <property name="right_attach">2</property>
            <property name="top_attach">1</property>
            <property name="x_options"/>
          </packing>
        </child>
      </object>
    </child>
  </object>
</interface>
