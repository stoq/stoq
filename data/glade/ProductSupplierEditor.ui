        <child>
          <object class="GtkLabel" id="label2">
            <property name="label" translatable="yes">Supplier Cost:</property>
            <property name="visible">True</property>
            <property name="xalign">1.0</property>
          </object>
          <packing>
            <property name="y_options"/>
